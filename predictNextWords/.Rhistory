df = data.frame(AIRCRAFT = c(741,741,742,742,743,743), ITEM = c("aa", "as", "qw", "wq", "rt", "rrr"), HOURS = c(10, NA, NA, 300, 400, 500),
DATE= c(NA, "2015-07-01","2015-10-01",NA,NA,NA))
aircrafts <- c('741','742','743','744','745','746','747','749','751','752',
'296','300','303','723','948')
updateTable <- function(h,...) {
file = subset(df, select =  c(AIRCRAFT, ITEM, HOURS, DATE))
file$AIRCRAFT = as.factor(file$AIRCRAFT)
file$HOURS = as.numeric(file$HOURS)
file$DATE = as.Date(as.character(file$DATE))
aircraft = svalue(checkbox.check)
lower = svalue(lower)
upper = svalue(upper)
from_d = as.Date(svalue(from_date))
to_d = as.Date(svalue(to_date))
temp = subset(file, ((AIRCRAFT %in% aircraft) & (HOURS >= lower & HOURS <= upper)) | ((AIRCRAFT %in% aircraft) & (DATE>=from_d & DATE<=to_d )))
df = temp
df$DATE = as.character(df$DATE)
t1[] <- df
}
saveFile <- function(h,...) {
file = subset(df, select =  c(AIRCRAFT, ITEM, HOURS, DATE))
file$AIRCRAFT = as.factor(file$AIRCRAFT)
file$HOURS = as.numeric(file$HOURS)
file$DATE = as.Date(file$DATE)
aircraft = svalue(checkbox.check)
lower = svalue(lower)
upper = svalue(upper)
from_d = as.Date(svalue(from_date), format = "%Y-%m-%d")
to_d = as.Date(svalue(to_date), format = "%Y-%m-%d")
temp = subset(file, ((AIRCRAFT %in% aircraft) & (HOURS >= lower & HOURS <= upper)) | ((AIRCRAFT %in% aircraft) & (DATE>=from_d & DATE<=to_d )))
df = temp
write.csv(df, file = "LOL")
}
